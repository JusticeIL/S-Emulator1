<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" spacing="20.0" stylesheets="@../css/aviad.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TopComponentController">
    <children>
        <Button fx:id="loadFileBtn" mnemonicParsing="false" onAction="#loadProgramPressed" text="Load program" />
      <Label fx:id="currentLoadedProgramPath" text="---" />
    </children>
    <padding>
        <Insets bottom="30.0" left="50.0" right="30.0" top="30.0" />
    </padding>
</HBox>
