<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<SplitPane dividerPositions="0.5" orientation="VERTICAL" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dashboard.controller.RightSideController">
  <items>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
         <children>
            <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <TableView fx:id="programsTable" prefHeight="200.0" prefWidth="200.0">
                      <placeholder>
                          <Label text="No programs uploaded." />
                      </placeholder>
                    <columns>
                      <TableColumn maxWidth="1000.0" minWidth="90.0" prefWidth="150.0" text="Program Name" />
                      <TableColumn maxWidth="1000.0" minWidth="90.0" prefWidth="150.0" text="User Uploading" />
                        <TableColumn maxWidth="100.0" minWidth="-Infinity" prefWidth="75.0" text="Insrtuctions" />
                        <TableColumn maxWidth="75.0" minWidth="-Infinity" prefWidth="60.0" text="Max Level" />
                        <TableColumn maxWidth="110.0" minWidth="-Infinity" prefWidth="100.0" text="Times Executed" />
                        <TableColumn maxWidth="150.0" minWidth="-Infinity" prefWidth="125.0" text="Average Credit Cost" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0" StackPane.alignment="BOTTOM_RIGHT">
                     <children>
                        <Button fx:id="executeProgramBtn" mnemonicParsing="false" onAction="#executeProgram" text="Execute Program" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0" />
                     </children>
                  </AnchorPane>
               </children>
            </StackPane>
         </children></AnchorPane>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
         <children>
            <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <TableView fx:id="functionsTable" prefHeight="200.0" prefWidth="200.0">
                      <placeholder>
                          <Label text="No functions uploaded." />
                      </placeholder>
                    <columns>
                      <TableColumn maxWidth="1000.0" minWidth="90.0" prefWidth="150.0" text="Function Name" />
                      <TableColumn maxWidth="500.0" minWidth="90.0" prefWidth="200.0" text="Program Origin" />
                        <TableColumn maxWidth="150.0" prefWidth="95.0" text="User Uploading" />
                        <TableColumn maxWidth="100.0" minWidth="-Infinity" prefWidth="75.0" text="Instructions" />
                        <TableColumn maxWidth="125.0" minWidth="-Infinity" prefWidth="65.0" text="Max Level" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0" StackPane.alignment="BOTTOM_RIGHT">
                     <children>
                        <Button fx:id="executeFunctionBtn" mnemonicParsing="false" onAction="#executeFunction" text="Execute Function" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0" />
                     </children>
                  </AnchorPane>
               </children>
            </StackPane>
         </children></AnchorPane>
  </items>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</SplitPane>
